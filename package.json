{
  "name": "@tfso/njs-bankaccount-validation",
  "version": "1.13.1",
  "description": "Validation of IBAN and BBAN numbers and other country specific bank account formats. Extendable with custom validations",
  "main": "dist/src/index",
  "types": "dist/src/index",
  "scripts": {
    "clean": "rm -rf package-lock.json node_modules",
    "build": "bash ./build.sh",
    "mocha": "mocha --exit --recursive --timeout 10000 --require ts-node/register ./test/**/test*",
    "lint": "npx eslint src test",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier . --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "test": "npm run mocha",
    "cover": "nyc --reporter=html --reporter=text npm run mocha"
  },
  "dependencies": {
    "ibantools": "4.1.4",
    "lodash.defaultsdeep": "4.6.1"
  },
  "devDependencies": {
    "@types/chai": "4.2.21",
    "@types/lodash.defaultsdeep": "4.6.6",
    "@types/mocha": "10.0.0",
    "@types/sinon-chai": "3.2.5",
    "chai": "4.3.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
"eslint-config-tfso": "github:tfso/eslint-config-tfso",
    "mocha": "10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^3.1.1",
    "sinon-chai": "3.7.0",
    "source-map-support": "0.5.19",
    "ts-mocha": "10.0.0",
    "ts-node": "10.2.1",
    "typescript": "4.4.2"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html",
      "text"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tfso/njs-tfso-bankaccount-validation.git"
  },
  "keywords": [
    "bankaccount",
    "bank",
    "account",
    "validation",
    "iban",
    "bban"
  ],
  "author": "24SevenOffice",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tfso/njs-tfso-bankaccount-validation/issues"
  },
  "homepage": "https://github.com/tfso/njs-tfso-bankaccount-validation#readme",
  "files": [
    "dist",
    "src"
  ]
}
